{
  "name": "interference-cluster-app-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:nginx": "vite --mode nginx",
    "dev:jsonServer": "vite --mode jsonServer",
    "build": "vite build",
    "preview": "vite preview",
    "fetch-openapi": "curl -s $VITE_OPENAPI_URL -o ./openapi.json",
    "generate:models": "openapi-generator-cli generate -i ./openapi.json -g typescript-axios -o ./src/generated",
    "generate": "npm run fetch-openapi && npm run generate:models",
    "prebuild": "npm run generate",
    "predev": "npm run generate"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "howler": "^2.2.4",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.4",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-simple-typeahead": "^1.0.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.14.0",
    "@types/howler": "^2.2.12",
    "@types/lodash": "^4.17.10",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  }
}
